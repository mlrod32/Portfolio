<!DOCTYPE html>
<html>
  <head>
    <link rel="stylesheet" href="/css/styles.css" />
    <title> Blog </title>
  </head>

  <body>
    <div class = "container">
      <div class ="heading">
      <%- include('partials/header.js') %>
      </div>

        <h1 class= "bloghead"> Blog Posts </h1>
      <div id = "post">
        <% for(var i = 0; i < blog.length;i++){ %>
          <div id = "box">
          <h3> <%= blog[i].title %> </h3>
          <p class = "body"> <%= blog[i].body%> </p>
        </div>
          <% } %>
      </div>
    </div>
  </body>
</html>
